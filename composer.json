{
    "name": "magento/magento2-functional-tests-base",
    "description": "Magento 2 Functional Tests Base",
    "type": "magento2-test-component",
    "version": "dev-master",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "config": {
        "secure-http": false
    },
    "repositories": [
        {
            "type" : "composer",
            "url" : "http://localhost:8080/"
        }
    ],
    "require": {
        "allure-framework/allure-codeception": "dev-master#af40af5ae2b717618a42fe3e137d75878508c75d",
        "codeception/codeception": "~2.3.4",
        "consolidation/robo": "^1.0.0",
        "symfony/process": ">=2.7 <3.4",
        "henrikbjorn/lurker": "^1.2",
        "magento/magento2-functional-test-framework": "dev-develop",
        "php": "7.0.2|7.0.4|~7.0.6|~7.1.0",
        "vlucas/phpdotenv": "~2.4",
        "magento/composer": "~1.2.0",
        "my/magento-composer-installer": "dev-master",
        "composer/composer": "1.4.1"
    },
    "autoload": {
        "psr-4": {
            "Magento\\": "tests/functional/Magento"
        }
    },
    "extra": {
        "map": [
            [
                ".gitignore",
                "dev/tests/acceptance/.gitignore"
            ],
            [
                ".env.example",
                "dev/tests/acceptance/.env.example"
            ],
            [
                "codeception.dist.yml",
                "dev/tests/acceptance/codeception.dist.yml"
            ],
            [
                "LICENSE.txt",
                "dev/tests/acceptance/LICENSE.txt"
            ],
            [
                "LICENSE_AFL.txt",
                "dev/tests/acceptance/LICENSE_AFL.txt"
            ],
            [
                "pre-install.php",
                "dev/tests/acceptance/pre-install.php"
            ],
            [
                "RoboFile.php",
                "dev/tests/acceptance/RoboFile.php"
            ],
            [
                "tests/_data",
                "dev/tests/acceptance/tests/_data"
            ],
            [
                "tests/_suite/sampleSuite.xml",
                "dev/tests/acceptance/tests/_suite/sampleSuite.xml"
            ],
            [
                "tests/functional.suite.dist.yml",
                "dev/tests/acceptance/tests/functional.suite.dist.yml"
            ],
            [
                "tests/_bootstrap.php",
                "dev/tests/acceptance/tests/_bootstrap.php"
            ],
            [
                "tests/functional/_bootstrap.php",
                "dev/tests/acceptance/tests/functional/_bootstrap.php"
            ]
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
